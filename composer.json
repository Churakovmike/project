{
    "name": "churakovmike/project",
    "type": "library",
    "description": "Наш пакет для презентации",
    "keywords": [
        "packages"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Churakov Mike",
            "role": "developer",
            "email": "churakovmike@mail.ru",
            "homepage": "https://github.com/Churakovmike"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "autoload": {
        "psr-4": {
            "ChurakovMike\\Project\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ChurakovMike\\Project\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.13"
    }
}
